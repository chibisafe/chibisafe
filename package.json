	"eslintConfig": {
		"extends": [
			"aqua/node",
			"aqua/vue"
		],
		"parserOptions": {
			"parser": "babel-eslint",
			"sourceType": "module"
		},
		"settings": {
			"import/resolver": {
				"nuxt": {
					"nuxtSrcDir": "./src/site",
					"extensions": [
						".js",
						".vue"
					]
				}
			}
		}
	},
	"nodemonConfig": {
		"watch": [
			"src/api/*"
		],
		"delay": 2500
	},
	"keywords": [
		"chibisafe",
		"lolisafe",
		"upload",
		"uploader",
		"file",
		"vue",
		"ssr",
		"file uploader",
		"images"
	],
